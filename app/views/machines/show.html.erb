<h1><%= @machine.location %> Vending Machine</h1>
<section id='stats'>
  <h4>Average Price: $<%= @average_price %></h4>
</section>
<h2>Snacks</h2>
<% @machine.snacks.each do |snack| %>
  <section id= "snack-<%= snack.id %>">
    <p>*
      <%= snack.name %>:
    $<%= snack.price %>
  </p>
  </section>
<% end %>
